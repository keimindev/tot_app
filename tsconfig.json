{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/_layout.tsx",
    "app/index.tsx",
    "context/GlobalProvider.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
